{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "DOM.Iterable", "ES6"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "strict": true,
    "module": "ES6",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "removeComments": true,
    "types": ["@types/chrome"]
  }
}
